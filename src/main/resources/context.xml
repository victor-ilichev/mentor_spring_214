<?xml version="1.0" encoding="utf-8" ?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-3.2.xsd
        http://www.springframework.org/schema/context
        http://www.springframework.org/schema/context/spring-context-3.2.xsd">

    <context:annotation-config />

<!--    <bean class="org.springframework.context.annotation.CommonAnnotationBeanPostProcessor" />-->
    <bean class="annotation.InjectRandomIntAnnotationBeanPostProcessor" />
    <bean class="quoter.TerminatorQuoter" id="terminatorQuoter">
        <property name="message" value="i`ll be back" />
    </bean>
    <bean class="annotation.ProfilingHandlerBeanPostProcessor" />
    <bean class="annotation.PostProxyInvokerContextListener" />

</beans>